import java.util.Scanner;
import java.io.*;
class posneg
{
  public static void main (String args[]) throws IOException
  {
   
      Scanner s = new Scanner (System.in);
      int n = s.nextInt();
      if(n>0)
        {
          System.out.println("Positive");
        }
      else if (n<0)
      {
        System.out.println("Negative");
       }
     else if (n==0)
     {
      System.out.println("Zero");
     }
 }
